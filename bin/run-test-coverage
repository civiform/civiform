#! /usr/bin/env bash

# DOC: Run the Java test suite in Docker.
# please open /server/jacoco/report/html/index.html to view the coverage result
source bin/lib.sh
docker::set_project_name_dev

bin/pull-image

docker::compose_dev up --wait -d
docker::dev_server_sbt_command jacoco
docker::compose_dev down --remove-orphans
